<?xml version="1.0" encoding="UTF-8"?>  

<widget xmins ="https://www.w3.org/ns/widgets"
xmins:gap= "http://phonegap.com/ns/1.0"
id= "com.phonegap.app5"
version = "1.0.0">

<name> appp5 Advertencias del dispositivo </name>

<description>
Aplicacion que realiza la vibracion y beep del dispositivo
</description>

<author href="https://build.phonegap.com" email="carina.cm813@gmail.com">
Carina Mart√≠nez Nieto
</author>

<!-- Perzonaliza la aplicacion y plataforma -->
<!-- <preference name="orientation" value="default"/> -->
<preference name="orientation" value="portrait"/>
<!-- <preference name="orientation" value="landscape2/> -->
<preference name="target-device" value="universal"/>
<preference name="fullscreen" value"true"/>
<preference name="android-installLocation" value"auto"/>
<preference name="permissions" value="INTERNET" />

<!-- Plugins -->
<!-- Core plugins  -->
<plugin name="cordova-plugin-device" spec="~1.1.4."/>
<plugin name="cordova-plugin-vibration" spec="~2.1.3"/>
<plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>

<!-- <plugin name="cordova-plugin-geolocation" source="npm" /> -->
<!-- Third part plugins -->

 <!-- Colorcar icono en la app-->
 
 <icon src="res/android/Custom-Icon-Design-Pretty-Office-8-Sound-on.ico"/>
 
 </widget>

